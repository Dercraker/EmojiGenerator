// This is your Prisma schema file
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Emoji {
  id              String   @id @default(cuid())
  slug            String   @unique
  prompt          String
  originalUrl     String
  createdAt       DateTime @default(now())


  @@index([prompt])
}
